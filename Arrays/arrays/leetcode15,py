nums = [-1, 0, 1, 2, -1, -4]
n = len(nums)
s = set()

for i in range(n):
    for j in range(i+1, n):
        a = 0 - (nums[i] + nums[j])
        if a in nums:
            # Ensure all indices are different
            k = nums.index(a)
            if k != i and k != j:
                s.add(tuple(sorted([nums[i], nums[j], a])))

print([list(x) for x in s])
